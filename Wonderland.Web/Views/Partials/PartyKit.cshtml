@inherits UmbracoViewPage<PartyKit>

<h1>Party Kit (partial) used on PistePoints and FundraisingIdeas pages</h1>

<dl>
    <dt>Heading</dt>
    <dd>@Model.Heading</dd>

    <dt>Images</dt>
    <dd>
        <ul>
            @foreach (Image image in Model.Images)
            {
                <li>
                    @image.Name
                    <img src="@image.Url" alt="@image.HtmlAltText" />
                </li>
            }

        </ul>
    </dd>

    <dt>Link Text</dt>
    <dd>@Model.LinkText</dd>

    <dt>Copy</dt>
    <dd>@Model.Copy</dd>
</dl>
